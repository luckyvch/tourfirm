<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-
//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.softserve.domain.Visas" table="visas">
	<id name="idVisa" column="idVisa" type="java.lang.Integer">
		<generator class="increment"/>
	</id>
	<property name="idClient" column="idClient" type="java.lang.Integer"/>
	<property name="dateOfIssue" column="dateOfIssue" type="java.util.Date"/>	
</class>
</hibernate-mapping>
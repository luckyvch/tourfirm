<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.softserve.domain.City" table="cities">
	<id name="idCity" column="idCity" type="java.lang.Integer">
		<generator class="increment"/>
	</id>
	<property name="cityName" column="cityName" type="java.lang.String"/>
	<property name="country" column="sName" type="java.lang.String"/>
	<property name="trasportation" column="trasportation" type="java.lang.String"/>
</class>
</hibernate-mapping>